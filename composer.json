{
    "name": "innmind/http",
    "type": "library",
    "description": "Value Objects to abstract http messages",
    "keywords": ["http", "value object"],
    "homepage": "http://github.com/Innmind/Http",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Http/issues"
    },
    "require": {
        "php": "~8.4",
        "innmind/url": "dev-next",
        "innmind/immutable": "dev-next",
        "innmind/filesystem": "dev-next",
        "innmind/io": "dev-next",
        "innmind/time-continuum": "dev-next",
        "innmind/media-type": "dev-next",
        "innmind/validation": "dev-next",
        "ramsey/uuid": "~4.7",
        "innmind/ip": "dev-next",
        "innmind/mutable": "dev-next"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Http\\": "tests/"
        }
    },
    "require-dev": {
        "innmind/static-analysis": "^1.2.1",
        "innmind/black-box": "~6.5",
        "innmind/coding-standard": "~2.0",
        "symfony/process": "^6.2"
    }
}
