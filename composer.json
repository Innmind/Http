{
    "name": "innmind/http",
    "type": "library",
    "description": "Value Objects to abstract http messages",
    "keywords": ["http", "value object"],
    "homepage": "http://github.com/Innmind/Http",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Http/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/url": "~4.0",
        "innmind/immutable": "~5.7",
        "innmind/filesystem": "~7.0",
        "innmind/io": "~2.2",
        "innmind/time-continuum": "~3.0",
        "innmind/stream": "~4.0",
        "innmind/media-type": "^2.0.1",
        "ramsey/uuid": "~4.7"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Http\\": "tests/"
        }
    },
    "require-dev": {
        "psr/http-message": "^1.0",
        "phpunit/phpunit": "~10.2",
        "innmind/static-analysis": "^1.2.1",
        "innmind/black-box": "~5.5",
        "innmind/coding-standard": "~2.0",
        "symfony/process": "^6.2"
    }
}
